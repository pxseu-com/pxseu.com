{
	"name": "server---main",
	"version": "0.1.0",
	"repository": "https://github.com/pxseu/pxseu.com",
	"private": true,
	"main": "./dist/server.js",
	"scripts": {
		"compile": "rimraf dist && tsc --project tsconfig.server.json",
		"start": "node .",
		"dev": "cross-env NODE_ENV=development PORT=8080 nodemon -e ts --exec \"npm run compile && npm start\"",
		"prod": "cross-env NODE_ENV=production PORT=6001 npm start",
		"deploy": "npm run compile && npm run build",
		"build": "next build",
		"postbuild": "next-sitemap"
	},
	"dependencies": {
		"cross-env": "^7.0.2",
		"express": "^4.17.1",
		"express-rate-limit": "^5.1.3",
		"helmet": "^4.1.1",
		"morgan": "^1.10.0",
		"next": "10.0.0",
		"next-sitemap": "^1.2.20",
		"nprogress": "^0.2.0",
		"react": "16.14.0",
		"react-device-detect": "^1.14.0",
		"react-dom": "16.14.0",
		"react-particles-js": "^3.4.0",
		"rimraf": "^3.0.2",
		"webhook-discord": "^3.7.7"
	},
	"devDependencies": {
		"@types/express": "^4.17.8",
		"@types/node": "^14.14.6",
		"@types/react": "^16.9.56",
		"nodemon": "2.0.6",
		"ts-node": "^9.0.0",
		"typescript": "^4.0.5"
	}
}
